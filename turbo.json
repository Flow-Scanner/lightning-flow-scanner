{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["lib/**", "out/**", "dist/**", "oclif.manifest.json"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "vite:dist": {
      "dependsOn": ["build"],
      "outputs": ["dist/**"]
    }
  }
}